section	.text	
	global prsym
	extern sys_puts
	

prsym:					;void prsym(int idx, int iline)
	push ebp
	mov ebp, esp

	mov	ebx,[ebp+8]	;ebp = inx
	mov	ecx,[ebp+12]	;ecx = iline
	mov	edx,		;jump to alphabet[idx] that is 24*inx (2^3 + 2^4 (8+16=24*inx))
	

.INX_LOOP:
	

	mov	eax,[esp+4]		;get the char c into dl
	movzx	eax,al
	add	eax,'0'			;add ascii code for 0 to c
	
	pop ebp
	ret

section	.data
  extern alpabet
  extern SYMBOL_HEIGHT



